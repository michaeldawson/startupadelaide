
<!-- contact section -->
<section class="row-fluid">
  <div class="container">
    <div class="col-sm-12">
      <%= simple_form_for ContactMessage.new do |f| %>
        <%= f.input :name, label: "Your name" %>
        <%= f.input :email, label: "Your email"  %>
        <%= f.input :message, label: "Anything nice you have to say" %>
        <%= f.submit "Send" %>
      <% end %>
    </div>
  </div>
</section>

